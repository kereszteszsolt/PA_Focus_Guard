{
  "name": "Focus Guard",
  "version": "1.0.4",s
  "description": "Focus Guard reminds you to focus when you try to access distracting websites by redirecting you to the Focus page.",
  "permissions": [
    "declarativeNetRequestWithHostAccess",
    "storage",
    "tabs"
  ],
  "icons": {
    "128": "assets/icons/kzs-focus.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_popup": "html/popup.html",
    "default_title": "Focus Guard"
  },
  "options_ui": {
    "page": "html/options.html",
    "open_in_tab": true
  },
  "manifest_version": 3
}
